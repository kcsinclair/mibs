    SYNTAX Integer32 (0..32)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "chassis index"
    ::= { emuEntry  1001}

emuSlotId OBJECT-TYPE
    SYNTAX Integer32 (0..32)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "slot index"
    ::= { emuEntry  1002}

emuItemId OBJECT-TYPE
    SYNTAX Integer32 (0..32)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "port/channel index"
    ::= { emuEntry  1003}

