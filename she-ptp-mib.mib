				"
			INDEX { shePtpPortDSPortNumber, shePtpForeignMasterDSIndex }
			::= { shePtpForeignMasterDataSetTable 1 }

		
		ShePtpForeignMasterDataSetEntry ::=
			SEQUENCE { 
				shePtpForeignMasterDSIndex
					Unsigned32,
				shePtpForeignMasterDSNumberForeignRecords
					Integer32,
				shePtpForeignMasterDSClockIdentity
					OCTET STRING,
				shePtpForeignMasterDSPortNumber
					Integer32,
				shePtpForeignMasterDSAnnounceMessages
					Unsigned32,
				shePtpForeignMasterDSNetworkProtocol
					ShePtpAcceptableMasterNetworkProtocolType,
				shePtpForeignMasterDSProtocolAddressLength
					Integer32,
				shePtpForeignMasterDSProtocolAddress
					OCTET STRING
			 }

		shePtpForeignMasterDSIndex OBJECT-TYPE
			SYNTAX Unsigned32 (1..10)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Second Index for table, Count of foreign master entries on a port.
				Foreign Master Data set shall have a minimum capacity of 5 foreign master records"
			::= { shePtpForeignMasterDataSetEntry 1 }

		
		shePtpForeignMasterDSNumberForeignRecords OBJECT-TYPE
			SYNTAX Integer32 (0..5)
			MAX-ACCESS read-only
