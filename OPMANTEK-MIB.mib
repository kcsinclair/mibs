	::= { omkEventGroup 7 }

omkState OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION 
		"Is the state true or false, up/down, open/closed, etc."
	::= { omkEventGroup 8 }

omkStateful OBJECT-TYPE
