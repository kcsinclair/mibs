       isisPmMjInterface,
       isisPmMjSlaveEntity,
       isisPmMjAddrType,
       isisPmMjMtId
       }
    ::= { isisPmMjTable 1 }

  IsisPmMjEntry ::= SEQUENCE {
      isisPmMjInterface                IsisPmInterfaceId,
