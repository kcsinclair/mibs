               FROM HUAWEI-DEVICE-MIB
           docsIfDownChannelId, docsIfUpChannelId
               FROM DOCS-IF-MIB
           ifAdminStatus, ifOperStatus
               FROM HUAWEI-TC-MIB
           InetAddressType,InetAddress,InetAddressPrefixLength,InetPortNumber
               FROM INET-ADDRESS-MIB;
                 
