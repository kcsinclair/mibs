			{
			 normal(1),
			 fault(2),
			 localLoopback(3),
			 forbidden(4),
			 test(5),
			 remoteLoopback(6),
			 activing(10),
			 deactive(11),
			 block(12),
			 nolight(13),
			 innerLocalLoopback(14),
			 innerRemoteLoopback(15),
