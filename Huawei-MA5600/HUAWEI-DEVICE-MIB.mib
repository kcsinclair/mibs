			  confirm(5),
			  delete(6),
			  reset(7),
			  localLoopback(8),
			  remoteLoopback(9),
			  stopLoopback(11),
			  innerLocalLoopback(12),
			  innerRemoteLoopback(13),
			  rtuLocalLoopback(14),
			  rtuRemoteLoopback(15),
			  utopiaLocalLoopback(16),
			  afeLocalLoopback(17),
			  hybirdLocalLoopback(18),
			  noLoopBackNoTest(50),
			  noLoopBackTest(51),
			  localLoopBackNoTest(52),
			  localLoopBackTest(53),
			  remoteLoopBackNoTest(54),
			  remoteLoopBackTest(55),
