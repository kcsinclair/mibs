		hwClkMibObjects OBJECT IDENTIFIER ::= { hwClock 1 }


		hwClockInManageTable OBJECT-TYPE
			SYNTAX SEQUENCE OF HwClockInManageEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Clock Source Management Table.	
				This table is used to add or delete the clock source and query 
				the status of the clock source.
				The index of this table is hwClkInIndex, which is the clock source ID.
				"
			::= { hwClkMibObjects 1 }


		hwClockInManageEntry OBJECT-TYPE
			SYNTAX HwClockInManageEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Clock Source Management Table.	
				This table is used to add or delete the clock source and query 
				the status of the clock source.
				The index of this entry is hwClkInIndex, which is the clock source ID.
				"
			INDEX { hwClkInIndex }
			::= { hwClockInManageTable 1 }

			HwClockInManageEntry ::=
			SEQUENCE
			{
			hwClkInIndex
			Integer32,
			hwClkInPostion
			Integer32,
			hwClkInWorkMode
			INTEGER,
			hwClkInOperStatus
			INTEGER,
			hwClkInRowStatus
			RowStatus,
			hwClkInBitsImpedance
			INTEGER,
			hwClkInBitsType
			INTEGER,
			hwClkInQL
			INTEGER,
			hwClkInFaultRecord
			INTEGER,
			hwClkInClearFaultRecord
			INTEGER,       
			hwClkLockState
			INTEGER
			}


		hwClkInIndex OBJECT-TYPE
			SYNTAX Integer32 (0..9 | 255)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Clock source index: ranging from 0 to 9.  The value 255 indicates the local
				clock source.
				Range: 0-9 | 255
				"
			::= { hwClockInManageEntry 1 }


		hwClkInPostion OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The ifIndex of physical position of clock source. If the clock source 
				isn't a sysline clock, the value will be 0xFFFFFFFFFFFFFF.
				The first byte indicates the NO. of CESoP index.
				The second byte indicates the NO. of frame, 
				the third byte indicates the NO. of slot and
				the lowest byte indicates the port ID.
				The following shows how to use parameter during the get or set operation:
				0x 01, 00,00,00  Cesop ID;     0~1
				0x 02, 00,00,00  System clock as an external clock
				0x 03, 00,00,00  1588 clock as an system clock   
				0x 00, FF,00,00  Frame ID;     0~255
				0x 00, 00,FF,00  Slot  ID;     0~255
				0x 00, 00,00,FF  Port  ID;     0~255
				"
			::= { hwClockInManageEntry 2 }


		hwClkInWorkMode OBJECT-TYPE
			SYNTAX INTEGER
			{
			none(1),
			sysLine(2),
			cesLine(3),
			cesSrts(4),
			e1Bits0(5),
			e1Bits1(6),
			f2mhzBits0(7),
			f2mhzBits1(8),
			msuSdh(9),
			msuTdm(10),
			localSurge(11),
			xpm8k(12),
			gPON(13),
			pkt1588(14),
			pw(15),
			copa(16),
			adaptClock(17), 
			aCMClock(18),
			t1Bits0(19),
			t1Bits1(20),
			system(21),
			acr1588(22), 
			syncEth(23)
			}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Clock source type. 
				Includes the line clock source,  BITS clock source, and local surge source.				
				If it is line clock source of a CES port, the clock source type may be CES line or CES SRTS clock source.
				If it is BITS clock source, the clock source type may be 2 MHZ or EL clock source of BITS0, or 2 MHZ or EL clock source of BITS1.
				If the value is 1, it indicates that a clock source is not configured.
				Options:
				1.none(1)                       - indicates that clock source not configured
				2.sysLine(2)                    - indicates that line clock source that needs to specify frame/slot/port
				3.cesLine(3)                    - indicates that line_clk of a CES clock source
				4.cesSrts(4)                    - indicates that srts_clk of a CES clock source
				5.e1Bits0(5)                    - indicates that EL type of a BITS0 clock source
				6.e1Bits1(6)                    - indicates that EL type of a BITS1 clock source
				7.f2mhzBits0(7)                 - indicates that 2MHZ type of a BITS0 clock source
				8.f2mhzBits1(8)                 - indicates that 2MHZ type of a BITS1 clock source
				9.msuSdh(9)                     - indicates that sdh_clk of an MSU clock source
				10.msuTdm(10)                   - indicates that tdm_clk of an MSU clock source
				11.localSurge(11)               - indicates that local surge clock
				12.xpm8k(12)                    - indicates that xpm-8k type of a xpm-8k clock source
				13.gPON(13)                     - indicates that 2MHZ type of a clock source at H601GP1A board
				14.pkt1588(14)                  - indicates that frequency synchronization from 1588 
				15.pw(15)                       - indicates that Pseudo-Wire clock source
				16.copa(16)                     - indicates that EL clock source of H601COPA board
				17.adaptClock(17)               - indicates that clock adapt from CESoP 
				18.aCMClock(18)                 - indicates that ACM clock 
				19.t1Bits0(19)                  - indicates that TL type of a BITS0 clock source
				20.t1Bits1(20)                  - indicates that TL type of a BITS1 clock source
				21.system(21)                   - indicates that system clock source  		
				22.acr1588(22)                  - indicates that 1588ACR clock source
				23.syncEth(23)                  - indicates that syncEth clock source 				
				"
			::= { hwClockInManageEntry 3 }


		hwClkInOperStatus OBJECT-TYPE
			SYNTAX INTEGER
			{
			none(1),
			normal(2),
			failure(3),
			failureLoss(4),
			failureEFD(5)
			}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Clock source state.
				Options:
				1.none(1)        - indicates that the clock source index isn't configured
				2.normal(2)      - indicates that the state of the clock source is active
				3.failure(3)     - indicates that the state of the clock source fails and this leaf is not used on the OLT
				4.failureLoss(4) - indicates that the clock source fails due to signal loss
				5.failureEFD(5)  - indicates that the clock source fails due to frequency offset
				"
			::= { hwClockInManageEntry 4 }


		hwClkInRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Clock Source operation:
				A user can add a clock source or clear a clock source. 
				After a user adds a clock source,the clock works in trace mode. 
				After a user clears a clock source, the clock works in srystal mode.
				To set the position(frame,slot,port) and oper(4), these two operations 
				must be set together.
				Oper(6) can be set by itself. To set a BITS clock source,
				the frame,slot,port parameters are useless.
				Oper(6) is used to clear a clock source.
				"
			::= { hwClockInManageEntry 5 }


		hwClkInBitsImpedance OBJECT-TYPE
			SYNTAX INTEGER
			{
			bitsImpedance75ohm(1),
			bitsImpedance120ohm(2),   
			bitsImpedance110ohm(3),
			bitsImpedance100ohm(4),
			bitsImpedanceNone(255)
			}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"BITS clock impedance type.The impedance value must be set when the clock source is a bits clock.
				Options:
				1. bitsImpedance75ohm(1)	- indicates that the impedance of this BITS clock source is 75 ohm
				2. bitsImpedance120ohm(2)	- indicates that  the impedance of this BITS clock source is 120 ohm   
				3. bitsImpedance110ohm(3)	- indicates that  the impedance of this BITS clock source is 110 ohm
				4. bitsImpedance100ohm(4)	- indicates that  the impedance of this BITS clock source is 100 ohm
				5. bitsImpedanceNone(255)	- indicates that this clock source is not a BITS clock			
				"
			::= { hwClockInManageEntry 6 }

		hwClkInBitsType OBJECT-TYPE
			SYNTAX INTEGER
			{
			bitsType2MHz(1),
			bitsType2Mbps(2),
			bitsType64K(3),
			bitsType1544bps(4),
			bitsTypeNone(255)
			}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"BITS clock type: 
				Options:
				1.bitsType2MHz(1)       -  indicates that the BITS clock type is 2MHz
				2.bitsType2Mbps(2)      -  indicates that the BITS clock type is 2bps  
				3.bitsType64K(3)        -  indicates that the BITS clock type is 64K
				4.bitsType1544bps(4)  -  indicates that the BITS clock type is bitsType1.544Mbps
				5.bitsTypeNone(255) -  indicates that  the clock source is not a BITS clock
				The BITS clock type must be set when the clock source is a BITS clock.
				"
			::= { hwClockInManageEntry 7 }
			
		hwClkInQL OBJECT-TYPE
			SYNTAX INTEGER
			{
			qlPRC(2),
			qlSsuA(4),
			qlSsuB(8),
			qlSEC(11),
			qlDNU(15),
			qlCLEAR(255)
			}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Clock source quality level: 
				Options:
				1. qlPRC(2)     -  indicates that primary reference clock
				2. qlSsuA(4)    -  indicates that the clock signal comes from class I or class
				                   V sub-clock prescribed on G.812 protocol
				3. qlSsuB(8)    -  indicates that the clock signal comes from class VI sub-clock 
				                   prescribed on G.812 protocol
				4. qlSEC(11)    -  indicates that the clock signal comes from SEC(SDH Equipment 
				                   Clock) prescribed on G.813 protocol
				5. qlDNU(15)    -  indicates that the clock signal can't be used for synchronization
				6. qlCLEAR(255) -  indicates that it is a wrong clock quality level
				"				 
			::= { hwClockInManageEntry 8 }
			
		hwClkInFaultRecord OBJECT-TYPE
			SYNTAX INTEGER
			{
			noFaultRecord(1),
			faultRecord(2)
			}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Clock source fault record.
				Options:
				1.noFaultRecord(1)  -  indicates that there is no fault record
				2.faultRecord(2)    -  indicates that the clock source had a fault record
				"			
			::= { hwClockInManageEntry 9 }
			
		hwClkInClearFaultRecord OBJECT-TYPE
			SYNTAX INTEGER
			{
			clearFaultRecord(1)
			}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Clear fault record of one clock source.
				Options:
				1.clearFaultRecord(1)  -Clears fault records
				"
			::= { hwClockInManageEntry 10 }
			
		hwClkLockState OBJECT-TYPE
			SYNTAX INTEGER
			{
			locked(1),
			unlocked(2)				
			}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Clock lock state.
				Options:
				1.locked(1)   - indicates that the clock is locked
				2.unlocked(2) - indicates that the clock is unlocked
				"
      		::= { hwClockInManageEntry 11 }

		hwClockOutManageTable OBJECT-TYPE
			SYNTAX SEQUENCE OF HwClockOutManageEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Clock Out Management Table. 
				This table is used to configure and query the priority of the clock source, 
				and query the index of the currently-output clock source, and the working mode (the output mode)
				of the current clock.The indexes of this table are hwClkInIndex and hwClkOutIndex. 
				hwClkInIndex comes from hwClockInManageTable and is the index of the input clock source.
				hwClkOutIndex is the type of the output clock source. 
				The output clock source can be of the SDH, TDM, 8K, or 19M type.
				"
			::= { hwClkMibObjects 2 }


		hwClockOutManageEntry OBJECT-TYPE
			SYNTAX HwClockOutManageEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Clock Out Management Table. 
				This table is used to configure and query the priority of the clock source, 
				and query the index of the currently-output clock source, and the working mode (the output mode)
				of the current clock.The indexes of this entry are hwClkInIndex and hwClkOutIndex. 
				hwClkInIndex comes from hwClockInManageTable and is the index of the input clock source.
				hwClkOutIndex is the type of the output clock source. 
				The output clock source can be of the SDH, TDM, 8K, or 19M type.
				"
			INDEX { hwClkInIndex, hwClkOutIndex }
			::= { hwClockOutManageTable 1 }

		HwClockOutManageEntry ::=
			SEQUENCE 
			{
			hwClkOutIndex
			Integer32,
			hwClkOutType
			INTEGER,
			hwClkOutPriority
			Integer32,
			hwClkOutWorkMode
			INTEGER,
			hwClkOutOperStatus
			TruthValue
			}


		hwClkOutIndex OBJECT-TYPE
			SYNTAX Integer32 (1..7)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Index of clock out table. Its meaning is the same as hwClkOutType.
				The clock out index must be 8k in UA system and must be SDH or
				TDM in MD/MA system during the setting of priority for a clock source.
				Range: 1 - 7
				"
			::= { hwClockOutManageEntry 1 }


		hwClkOutType OBJECT-TYPE
			SYNTAX INTEGER
			{
			sdh(1),
			tdm(2),
			clk8K(3),
			clk19M(4),
			system(5),
			outputClk0(6),
			outputClk1(7)
			}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Clock out type.
				Currently, there are seven types: SDH, TDM, 8 kHz, 19 MHzm, system, outputClk0 and outputClk1.
				Options:
				1.sdh(1)        -   indicates that output clock type is SDH
				2.tdm(2)        -   indicates that output clock type is TDM
				3.clk8K(3)      -   indicates that output clock type is clk8K       
				4.clk19M(4)     -   indicates that output clock type is clk19M       
				5.system(5)     -   indicates that output clock type is system      
				6.outputClk0(6) -   indicates that output clock type is outputClk0       
				7.outputClk1(7) -   indicates that output clock type is outputClk1      
