    InterfaceIndex
        FROM IF-MIB
    IfDirection
        FROM DOCS-QOS3-MIB

    cadCable
        FROM CADANT-PRODUCTS-MIB

    CardId,
    AdminState
        FROM CADANT-TC

    cadOvSysCpuStatus
        FROM CADANT-CMTS-PROCESS-MIB

