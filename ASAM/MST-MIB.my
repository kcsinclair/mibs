dot1sMstTrapsControl OBJECT IDENTIFIER
    ::= { mstMIB 2 }

mstGenTrapType OBJECT-TYPE
    SYNTAX      INTEGER 
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The general events like 
         0:none - none of the below values
         1:up - protocol UP,
         2:down - protocol DOWN.
	 others:undefined"
    ::= { dot1sMstTrapsControl 1 }

mstErrTrapType OBJECT-TYPE
    SYNTAX      INTEGER 
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The error event like
         0:none - none of the below values
         1:memfail - memory allocation failure
         2:bufffail - buffer allocation failure.
	 others:undefined"
    ::= { dot1sMstTrapsControl 2 }

mstPortTrapIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "A unique value, greater than zero, indicating the Port number."
    ::= { dot1sMstTrapsControl 3 }

mstPortMigrationType OBJECT-TYPE
    SYNTAX      INTEGER 
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Port Protocol migration type occured in the port
	0:sendstp
	1:sendrstp
	others:undefined"
    ::= { dot1sMstTrapsControl 4 }

mstPktErrType OBJECT-TYPE
    SYNTAX      INTEGER 
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
        "Type of invalid packet received in each of the port
         0:protocolIdErr
	 1:invalidBpdu
	 2:configLengthErr
	 3:tcnLengthErr
	 4:rstpLengthErr
	 5:maxAgeErr
	 6:fwdDelayErr
	 7:helloTimeErr
	 8:mstpLengthErr
	 others:undefined"
    ::= { dot1sMstTrapsControl 5 }

mstPktErrVal OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Packet error value corresponding to the above type"	 
    ::= { dot1sMstTrapsControl 6 }

