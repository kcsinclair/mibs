mstMstiPortTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF MstMstiPortEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "This table contains Spanning Tree Instance Specific Port
         Information.
         ALCATEL NOTE:
          TABLESIZE: DEP
         DESCR: DEP(TABLESIZE)	The range of this table is the number of msti instances
         			multiplied by the number of ports. "
    ::= { dot1sMst 34 }

mstMstiPortEntry OBJECT-TYPE
    SYNTAX      MstMstiPortEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "A list of information maintained by every port for each 
         and every spanning tree instance.
         Row creation : An mstMstiPortEntry will be created for all the Member 
         		Ports associated with the VLAN of an MST Instance.
         Row deletion : An mstMstiPortEntry will be deleted if the Member 
         		port is disassociated from the VLAN of an MST Instance or 
         		when the MST Instance is deleted.
         Index usage : the index is mstMstiPort and mstInstanceIndex.
         Note that entries will be presented to the SNMP manager in full
         lexicographical order :  sorted on mstMstiPort, then on mstInstanceIndex."
    INDEX       { mstMstiPort, mstInstanceIndex }
    ::= { mstMstiPortTable 1 }

MstMstiPortEntry ::= SEQUENCE {
    mstMstiPort                           InterfaceIndex,
    mstMstiPortInstanceIndex              INTEGER,
    mstMstiPortPathCost                   INTEGER,
    mstMstiPortPriority                   INTEGER,
    mstMstiPortDesignatedRoot             BridgeId,
    mstMstiPortDesignatedBridge           BridgeId,
    mstMstiPortDesignatedPort             OCTET STRING,
    mstMstiPortState                      INTEGER,
    mstMstiForcePortState                 INTEGER,
    mstMstiPortForwardTransitions         Counter,
    mstMstiPortReceivedBPDUs              Counter,
    mstMstiPortTransmittedBPDUs           Counter,
    mstMstiPortInvalidBPDUsRcvd           Counter,
    mstMstiPortDesignatedCost             INTEGER,
    mstMstiSelectedPortRole               INTEGER,
    mstMstiCurrentPortRole                INTEGER,
    mstMstiPortInfoSemState               INTEGER,
    mstMstiPortRoleTransitionSemState     INTEGER,
    mstMstiPortStateTransitionSemState    INTEGER,
    mstMstiPortTopologyChangeSemState     INTEGER
}

mstMstiPort OBJECT-TYPE
    SYNTAX      InterfaceIndex
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "The Port number of the port for which this entry contains   
         spanning tree information.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     YES          INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { mstMstiPortEntry 1 }

mstMstiPortInstanceIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..64)
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "An arbitrary integer within the range from 1 to the value of
         Max Instance Number that uniquely identifies an instance.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     YES          INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { mstMstiPortEntry 2 }
    
mstMstiPortPathCost OBJECT-TYPE
    SYNTAX      INTEGER (1..200000000)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The contribution of this port to the path cost of
         paths towards the MSTI Root which include this port.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     YES          INSRVMOD:    YES
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { mstMstiPortEntry 3 }

mstMstiPortPriority OBJECT-TYPE
    SYNTAX      INTEGER (0..240)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The four most significant bits of the Port Identifier 
         for a given Spanning Tree instance can be modified 
         independently for each Spanning Tree instance 
         supported by the Bridge. The values that are set for Port 
         Priority must be in steps of 16.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     YES          INSRVMOD:    YES
           RANGE:       NA           DEFVALUE:    NA
           UNITS:       NA           SPARSE:      NO
           DESCR:       NA"
    DEFVAL      { 128 }
    ::= { mstMstiPortEntry 4 }

mstMstiPortDesignatedRoot OBJECT-TYPE
    SYNTAX      BridgeId
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The unique Bridge Identifier of the bridge recorded as the  
         MSTI Regional Root in the configuration BPDUs transmitted.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { mstMstiPortEntry 5 }

mstMstiPortDesignatedBridge OBJECT-TYPE
    SYNTAX      BridgeId
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The unique Bridge Identifier of the bridge which this port  
         considers to be the Designated Bridge for the port's segment.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { mstMstiPortEntry 6 }

mstMstiPortDesignatedPort OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(2))
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The Port identifier of the port on the Designated Bridge    
         for this port's segment.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { mstMstiPortEntry 7 }

mstMstiPortState OBJECT-TYPE
    SYNTAX      INTEGER { disabled(1), discarding(2), learning(4), 
                    forwarding(5) }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Current state of the Port as defined by the Multiple  
         spanning tree protocol. Port which is Forwarding state
         in one instance can be in Discarding (Blocking) state 
         in another instance.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    NA
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { mstMstiPortEntry 8 }

mstMstiForcePortState OBJECT-TYPE
    SYNTAX      INTEGER { disabled(0), enabled(1) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Current state of the Port which can be changed to either 
         Disabled or Enabled for the specific spanning tree   
         instance.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     YES          INSRVMOD:    YES
          RANGE:       NA           DEFVALUE:    DEP
          UNITS:       NA           SPARSE:      NO
          DESCR:       Default value shall be of mstCistForcePortState during Msti port entry creation"
    DEFVAL      { disabled }
    ::= { mstMstiPortEntry 9 }

mstMstiPortForwardTransitions OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Number of times this port has transitioned to the     
         Forwarding State for specific instance.
         ALCATEL NOTE:
          ACCESS:      NA           USAGE:       NA
          PERSIST:     NO           INSRVMOD:    NA
          RANGE:       NA           DEFVALUE:    0
          UNITS:       NA           SPARSE:      NO
          DESCR:       NA"
    ::= { mstMstiPortEntry 10 }

mstMstiPortReceivedBPDUs OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Number of BPDUs received by this port for this 
         spanning tree instance.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     NO           INSRVMOD:    NA
           RANGE:       NA           DEFVALUE:    0
           UNITS:       BPDU         SPARSE:      NO
           DESCR:       NA"
    ::= { mstMstiPortEntry 11 }

mstMstiPortTransmittedBPDUs OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Number of BPDUs transmitted on this port for 
         this spanning tree instance.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     NO           INSRVMOD:    NA
           RANGE:       NA           DEFVALUE:    0
           UNITS:       BPDU         SPARSE:      NO
           DESCR:       NA"
    ::= { mstMstiPortEntry 12 }

mstMstiPortInvalidBPDUsRcvd OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
