
-- orig MST-MIB

MST-MIB DEFINITIONS ::= BEGIN

IMPORTS
    BridgeId, Timeout
        FROM BRIDGE-MIB
    InterfaceIndex
        FROM ITF-MIB
    VlanIndex
        FROM Q-BRIDGE-MIB
    asam                    
        FROM SYSTEM-MIB
    OBJECT-TYPE
        FROM RFC-1212
    TRAP-TYPE
        FROM RFC-1215
    Counter, TimeTicks
        FROM RFC1155-SMI
    MacAddress, RowStatus, TruthValue
        FROM SNMPv2-TC;

mstMIB OBJECT IDENTIFIER
    ::= { asam 90 }

EnabledStatus ::=
    INTEGER { enabled(1), disabled(2) }

dot1sMst OBJECT IDENTIFIER
    ::= { mstMIB 1 }

mstModuleStatus OBJECT-TYPE
    SYNTAX      EnabledStatus
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The administrative status requested by management for the MST  
         feature. The value enabled(1) indicates that Mst should be enabled  
         in the device on all ports. The value disabled(2) indicates that 
         Mst should be disabled in the device on all ports. 
         	ALCATEL NOTE:
               ACCESS:      NA           USAGE:       NA
               PERSIST:     YES          INSRVMOD:    YES
               RANGE:       1,2          DEFVALUE:    NA
               UNITS:       NA           SPARSE:      NA
               DESCR:       NA"
    DEFVAL      { enabled }
    ::= { dot1sMst 1 }

mstMaxMstInstanceNumber OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The Maximun number of spanning trees to be allowed.
         A User may limit the Number of Spanning Tree instance 
         to be allowed in the Bridge.
         	ALCATEL NOTE:
              ACCESS:      NA           USAGE:       NA
              PERSIST:     YES          INSRVMOD:    YES
              RANGE:       DEP          DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NA
              DESCR:       mstMaxMstInstanceNumber should not be less than
         			 mstNoOfMstiSupported."
    DEFVAL      { 65 }
    ::= { dot1sMst 2 }

mstNoOfMstiSupported OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Indicates Maximum number of spanning tree Instances supported.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       0..64        DEFVALUE:    0
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    ::= { dot1sMst 3 }

mstNextFreeMSTiIndex OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "A index to indicate the next free entry in the MSTI Bridge Table
         ALCATEL NOTE:
            ACCESS:      NA           								USAGE:       NA
            PERSIST:     NO           								INSRVMOD:    NA
            RANGE:       1..(mstMaxMstInstanceNumber-1)   DEFVALUE:    1
            UNITS:       NA           								SPARSE:      NA
            DESCR:       When there are no instances free the value returned 
         							 will be mstMaxMstInstanceNumber"
    ::= { dot1sMst 4 }

mstMaxHopCount OBJECT-TYPE
    SYNTAX      INTEGER (600..4000)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Indicates the Maximum Hop Count value.
         The granularity of this timer is specified 
         to be 1 second.  An agent may return a badValue 
         error if a set is attempted to a value which is 
         not a whole number of seconds.
         	ALCATEL NOTE:
              ACCESS:      NA           		USAGE:       NA
              PERSIST:     YES          		INSRVMOD:    YES
              RANGE:       NA           		DEFVALUE:    NA
              UNITS:       1/100th of Second      SPARSE:      NA
              DESCR:       NA"
    DEFVAL      { 2000 }
    ::= { dot1sMst 5 }

mstBrgAddress OBJECT-TYPE
    SYNTAX      MacAddress
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The MAC address used by this bridge when it must
         be referred to in a unique fashion.   It is
         recommended that this be the numerically smallest
         MAC address of all ports that belong to this
         bridge.  However it is only required to be unique.
         When concatenated with mstCistBridgePriority or 
         mstMstiBridgePriority a unique BridgeIdentifier 
         is formed which is used in the Spanning Tree Protocol.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     NO           INSRVMOD:    NA
           RANGE:       NA           DEFVALUE:    NA
           UNITS:       NA           SPARSE:      NA
           DESCR:       NA"
    ::= { dot1sMst 6 }

mstCistRoot OBJECT-TYPE
    SYNTAX      BridgeId
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The bridge identifier of the Root of the common spanning
         tree as determined by the Spanning Tree Protocol
         as executed by this node.  This value is used as
         the CIST Root Identifier parameter in all Configuration
         Bridge PDUs originated by this node.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    ::= { dot1sMst 7 }

mstCistRegionalRoot OBJECT-TYPE
    SYNTAX      BridgeId
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The bridge identifier of the Root of the Multiple
         spanning tree region as determined by the Spanning Tree
         Protocol as executed by this node.  This value is used as
         the CIST Regional Root Identifier parameter in all Configuration
         Bridge PDUs originated by this node.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    ::= { dot1sMst 8 }

mstCistRootCost OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The Cost of the path to the CIST Root as seen 
         from this bridge.
         	ALCATEL NOTE:
             ACCESS:      NA           USAGE:       NA
             PERSIST:     NO           INSRVMOD:    NA
             RANGE:       NA           DEFVALUE:    NA
             UNITS:       NA           SPARSE:      NA
             DESCR:       NA"
    ::= { dot1sMst 9 }

mstCistRegionalRootCost OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The Cost of the path to the CIST Regional Root 
         as seen from this bridge.
         	ALCATEL NOTE:
             ACCESS:      NA           USAGE:       NA
             PERSIST:     NO           INSRVMOD:    NA
             RANGE:       NA           DEFVALUE:    NA
             UNITS:       NA           SPARSE:      NA
             DESCR:       NA"
    ::= { dot1sMst 10 }

mstCistRootPort OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The Port Number of the Port which offers the lowest    
         path cost from this bridge to the CIST Root Bridge.
         	ALCATEL NOTE:
              ACCESS:      NA           USAGE:       NA
              PERSIST:     NO           INSRVMOD:    NA
              RANGE:       DEP        DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NA
              DESCR:       DEP(RANGE): This object will return 0 when it  is acting as a Root Bridge.
         		In all other cases it will return the Root Port index"
    ::= { dot1sMst 11 }

mstCistBridgePriority OBJECT-TYPE
    SYNTAX      INTEGER (0..61440)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The Value of the writable portion of the Bridge
         Identifier comprising of the first two octets.
         The values that are set for Bridge Priority must be 
         in steps of 4096.
         	ALCATEL NOTE:
             ACCESS:      NA           USAGE:       NA
             PERSIST:     YES          INSRVMOD:    YES
             RANGE:       NA           DEFVALUE:    NA
             UNITS:       NA           SPARSE:      NA
             DESCR:       NA"
    DEFVAL      { 32768 }
    ::= { dot1sMst 12 }

mstCistBridgeMaxAge OBJECT-TYPE
    SYNTAX      Timeout 
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The value that all bridges use for MaxAge when
         this bridge is acting as the root.  Note that
         802.1D specifies that the range for this
         parameter is related to the value of
         BridgeHelloTime. The granularity of this
         timer is specified to be 1 second.
         An agent may return a badValue error if a set is
         attempted to a value which is not a whole number
         of seconds.
         ALCATEL NOTE:
            ACCESS:      NA           		USAGE:       NA
            PERSIST:     YES          		INSRVMOD:    YES
            RANGE:       NA           		DEFVALUE:    NA
            UNITS:       1/100th of Second      SPARSE:      NA
            DESCR:       mstCistBridgeMaxAge should be less than or equal to 
         		 (2 * ((mstCistBridgeForwardDelay/100)-1) and 
         		 mstCistBridgeMaxAge should be greater than or equal 
         		 to (2 * ((mstCistPortHelloTime/100) + 1))"
    DEFVAL      { 2000 }
    ::= { dot1sMst 13 }

mstCistBridgeHelloTime OBJECT-TYPE
    SYNTAX      Timeout 
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The value that all bridges use for HelloTime when
         this bridge is acting as the root.  The
         granularity of this timer is specified to be 1 second.  
         An agent may return a badValue error if a set is attempted 
         to a value which is not a whole number of seconds.
         ALCATEL NOTE:
            ACCESS:      NA           		USAGE:       NA
            PERSIST:     YES          		INSRVMOD:    YES
            RANGE:       NA           		DEFVALUE:    NA
            UNITS:       1/100th of Second      SPARSE:      NA
            DESCR:       (2 * ((mstCistBridgeHelloTime/100) + 1)) should be 
         		 less than or equal to mstCistBridgeMaxAge. This object can be set 
         		 only when mstForceProtocolVersion is stp(0) or rstp(2)"
    DEFVAL      { 200 }
    ::= { dot1sMst 14 }

mstCistBridgeForwardDelay OBJECT-TYPE
    SYNTAX      Timeout 
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The value that all bridges use for ForwardDelay
         when this bridge is acting as the root.  Note that
         802.1D specifies that the range for this
         parameter is related to the value of
         BridgeMaxAge.  The granularity of this
         timer is specified to be 1 second.
         An agent may return a badValue error if a set is
         attempted to a value which is not a whole number
         of seconds.
         ALCATEL NOTE:
            ACCESS:      NA           		USAGE:       NA
            PERSIST:     YES          		INSRVMOD:    YES
            RANGE:       NA           		DEFVALUE:    NA
            UNITS:       1/100th of Second      SPARSE:      NA
            DESCR:       (2 * ((mstCistBridgeForwardDelay/100) - 1) should be greater than 
         		 or equal to mstCistBridgeMaxAge."
    DEFVAL      { 1500 }
    ::= { dot1sMst 15 }

mstCistHoldTime OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "This time value determines the interval length
         during which no more than two Configuration bridge
         PDUs shall be transmitted by this node, in units
         of hundredths of a second.
         ALCATEL NOTE:
           ACCESS:      NA           		USAGE:       NA
           PERSIST:     NO           		INSRVMOD:    NA
           RANGE:       NA           		DEFVALUE:    NA
           UNITS:       1/100th of Second      SPARSE:      NA
           DESCR:       NA"
    ::= { dot1sMst 16 }

mstCistMaxAge OBJECT-TYPE
    SYNTAX      Timeout
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The maximum age of Spanning Tree Protocol
         information learned from the network on any port
         before it is discarded, in units of hundredths of
         a second.  This is the actual value that this
         bridge is currently using.
         ALCATEL NOTE:
           ACCESS:      NA           		USAGE:       NA
           PERSIST:     NO           		INSRVMOD:    NA
           RANGE:       NA           		DEFVALUE:    NA
           UNITS:       1/100th of Second      SPARSE:      NA
           DESCR:       NA"
    ::= { dot1sMst 17 }

mstCistHelloTime OBJECT-TYPE
    SYNTAX      Timeout
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The amount of time between the transmission of
         Configuration bridge PDUs by this node on any port
         when it is the root of the spanning tree or trying
         to become so, in units of hundredths of a second.
         This is the actual value that this bridge is
         currently using.
         ALCATEL NOTE:
         ACCESS:      NA           		USAGE:       NA
         PERSIST:     NO           		INSRVMOD:    NA
         RANGE:       NA           		DEFVALUE:    NA
         UNITS:       1/100th of Second      SPARSE:      NA
         DESCR:       NA"
    ::= { dot1sMst 18 }

mstCistForwardDelay OBJECT-TYPE
    SYNTAX      Timeout
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "This time value, measured in units of hundredths
         of a second, controls how fast a port changes its
         spanning state when moving towards the Forwarding
         state.  The value determines how long the port
         stays in a particular state before moving to the
         next state.
         ALCATEL NOTE:
         ACCESS:      NA           		USAGE:       NA
         PERSIST:     NO           		INSRVMOD:    NA
         RANGE:       NA           		DEFVALUE:    NA
         UNITS:       1/100th of Second      SPARSE:      NA
         DESCR:       NA"
    ::= { dot1sMst 19 }

mstMstpUpCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times MSTP Module has been enabled.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    0
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    ::= { dot1sMst 20 }

mstMstpDownCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times MSTP Module has been disabled.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    0
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    ::= { dot1sMst 21 }

mstPathCostDefaultType OBJECT-TYPE
    SYNTAX      INTEGER { stp8021d1998(1), stp8021t2001(2) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The version of the Spanning Tree default Path Costs that
         are to be used by this Bridge.  A value of 8021d1998(1)
         uses the 16-bit default Path Costs from IEEE Std. 802.1D-1998.
         A value of stp8021t2001(2) uses the 32-bit default Path
         Costs from IEEE Std. 802.1t.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     YES          INSRVMOD:    YES
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    DEFVAL      { stp8021t2001 }
    ::= { dot1sMst 22 }

mstForceProtocolVersion OBJECT-TYPE
    SYNTAX      INTEGER { stpCompatible(0), rstp(2), mstp(3) }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The version of Spanning Tree Protocol the bridge is
         currently running.  The value 'stpCompatible(0)'
         indicates the Spanning Tree Protocol specified in
         IEEE 802.1D and 'rstp(2)' indicates the Rapid Spanning
         Tree Protocol specified in IEEE 802.1w and 'mstp(3)'
         indicates the Multiple Spanning Tree Protocol Specified
         in IEEE 802.1s.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     YES          INSRVMOD:    YES
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    DEFVAL      { mstp }
    ::= { dot1sMst 23 }

mstTxHoldCount OBJECT-TYPE
    SYNTAX      INTEGER (1..10)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The value used by the Port Transmit state machine to limit
         the maximum transmission rate.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     YES          INSRVMOD:    YES
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    DEFVAL      { 3 }
    ::= { dot1sMst 24 }

mstMstiConfigIdSel OBJECT-TYPE
    SYNTAX      INTEGER (0..255)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The Configuration Identifier Format Selector used 
         by the Bridge. This has a fixed value of 0 to indicate
         RegionName, RegionVersion are specified as in Standard.
         	ALCATEL NOTE:
              ACCESS:      NA           USAGE:       NA
              PERSIST:     YES          INSRVMOD:    YES
              RANGE:       NA           DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NA
              DESCR:       NA"
    DEFVAL      { 0 }
    ::= { dot1sMst 25 }

mstMstiRegionName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The Name for the Region's configuration. By Default 
         Region Name will be equal to the Bridge Mac Address.
         	ALCATEL NOTE:
              ACCESS:      NA           USAGE:       NA
              PERSIST:     YES          INSRVMOD:    YES
              RANGE:       NA           DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NA
              DESCR:       NA"
    ::= { dot1sMst 26 }

mstMstiRegionVersion OBJECT-TYPE
    SYNTAX      INTEGER (0..65535)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Version of the MST Region.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     YES          INSRVMOD:    YES
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    DEFVAL      { 0 }
    ::= { dot1sMst 27 }

mstMstiConfigDigest OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The Configuration Digest value for this Region.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    NA
            UNITS:       NA           SPARSE:      NA
            DESCR:       NA"
    ::= { dot1sMst 28 }

mstRegionConfigChangeCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times a Region Configuration Identifier Change 
         was detected. 
         A Trap is generated on the occurence of this event.
         ALCATEL NOTE:
         ACCESS:      NA           USAGE:       NA
         PERSIST:     NO           INSRVMOD:    NA
         RANGE:       NA           DEFVALUE:    0
         UNITS:       NA           SPARSE:      NA
         DESCR:       NA"
    ::= { dot1sMst 29 }

mstCistNewRootBridgeCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times this Bridge has detected a Root
         Bridge change for Common Spanning Tree context.
         A Trap is generated on the occurence of this event.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     NO           INSRVMOD:    NA
           RANGE:       NA           DEFVALUE:    0
           UNITS:       NA           SPARSE:      NA
           DESCR:       NA"
    ::= { dot1sMst 30 }

mstMstiBridgeTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF MstMstiBridgeEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "Table containing Bridge Information specific to Spanning 
         Tree Instance.
          ALCATEL NOTE:
              TABLESIZE: 	64
         	  	DESCR: 	NA"
    ::= { dot1sMst 31 }

mstMstiBridgeEntry OBJECT-TYPE
    SYNTAX      MstMstiBridgeEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION 
        "Entry indicating the Bridge Information.
         Row creation : A mstMstiBridgeEntry entry shall be created in one 
         		        step by a set-request with mstMstiBridgeRowStatus 
         			set to createAndGo(4) or createAndWait(5).
         Row deletion : A mstMstiBridgeEntry entry shall be deleted by setting
             		the mstMstiBridgeRowStatus of the specific entry
         			to destroy(6).
         Index usage : the index is mstMstiBridgeInstanceIndex.
           Note that entries will be presented to the SNMP manager in full
           lexicographical order :  sorted on mstMstiBridgeInstanceIndex."
    INDEX       { mstMstiBridgeInstanceIndex }
    ::= { mstMstiBridgeTable 1 }

MstMstiBridgeEntry ::= SEQUENCE {
    mstMstiBridgeInstanceIndex                  INTEGER,
    mstMstiBridgeRegionalRoot             BridgeId,
    mstMstiBridgePriority                 INTEGER,
    mstMstiRootCost                       INTEGER,
    mstMstiRootPort                       InterfaceIndex,
    mstMstiNewRootBridgeCount             Counter,
    mstInstanceUpCount                    Counter,
    mstInstanceDownCount                  Counter,
    mstMstiBridgeRowStatus                RowStatus
}

mstMstiBridgeInstanceIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..64)
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "Spanning Tree Instance to which the information belongs.
         ALCATEL NOTE:
             ACCESS:      NA           USAGE:       NA
             PERSIST:     YES          INSRVMOD:    NA
             RANGE:       NA           DEFVALUE:    NA
             UNITS:       NA           SPARSE:      NO
             DESCR:       NA"
    ::= { mstMstiBridgeEntry 1 }

mstMstiBridgeRegionalRoot OBJECT-TYPE
    SYNTAX      BridgeId
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "MSTI Regional Root Identifier value for the Instance. This value 
         is used as the MSTI Regional Root Identifier parameter in all
         Configuration Bridge PDUs originated by this node
          ALCATEL NOTE:
              ACCESS:      NA           USAGE:       NA
              PERSIST:     NO           INSRVMOD:    NA
              RANGE:       NA           DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NO
              DESCR:       NA"
    ::= { mstMstiBridgeEntry 2 }

mstMstiBridgePriority OBJECT-TYPE
    SYNTAX      INTEGER (0..61440)
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "The writable portion of the MSTI Bridge Identifier.
         comprising of the first two octets.
         The values that are set for Bridge Priority must be 
         in steps of 4096.
          ALCATEL NOTE:
              ACCESS:      NA           USAGE:       OPT
              PERSIST:     YES          INSRVMOD:    YES
              RANGE:       NA           DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NO
              DESCR:       NA"
    DEFVAL      { 32768 }
    ::= { mstMstiBridgeEntry 3 }

mstMstiRootCost OBJECT-TYPE
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The Cost of the path to the MSTI Regional Root as seen 
         by this bridge.
          ALCATEL NOTE:
              ACCESS:      NA           USAGE:       NA
              PERSIST:     NO           INSRVMOD:    NA
              RANGE:       NA           DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NO
              DESCR:       NA"
    ::= { mstMstiBridgeEntry 4 }

mstMstiRootPort OBJECT-TYPE
    SYNTAX      InterfaceIndex
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The Port Number of the Port which offers the lowest    
         path cost from this bridge to the MSTI Region Root Bridge.
          ALCATEL NOTE:
              ACCESS:      NA           USAGE:       NA
              PERSIST:     NO           INSRVMOD:    NA
              RANGE:      DEP        DEFVALUE:    NA
              UNITS:       NA           SPARSE:      NO
              DESCR:       DEP(RANGE): This object will return 0 when it  is acting as a Root Bridge.
         		In all other cases it will return the Root Port index"
    ::= { mstMstiBridgeEntry 5 }

mstMstiNewRootBridgeCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times this Bridge has detected a Root
         Bridge change for this spanning tree instance.
         A Trap is generated on the occurence of this event.
         ALCATEL NOTE:
           ACCESS:      NA           USAGE:       NA
           PERSIST:     NO           INSRVMOD:    NA
           RANGE:       NA           DEFVALUE:    0
           UNITS:       NA           SPARSE:      NO
           DESCR:       NA"
    ::= { mstMstiBridgeEntry 6 }

mstInstanceUpCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times a new spanning tree instance has
         been created.
         A Trap is generated on the occurence of this event.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    0
            UNITS:       NA           SPARSE:      NO
            DESCR:       NA"
    ::= { mstMstiBridgeEntry 7 }

mstInstanceDownCount OBJECT-TYPE
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION 
        "The number of times a spanning tree instance has
         been deleted.
         A Trap is generated on the occurence of this event.
         ALCATEL NOTE:
            ACCESS:      NA           USAGE:       NA
            PERSIST:     NO           INSRVMOD:    NA
            RANGE:       NA           DEFVALUE:    0
            UNITS:       NA           SPARSE:      NO
            DESCR:       NA"
    ::= { mstMstiBridgeEntry 8 }

mstMstiBridgeRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION 
        "Specifies the Row Status of the MSTI Bridge entry. 
