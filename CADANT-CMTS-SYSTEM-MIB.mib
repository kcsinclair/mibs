    DEFVAL { 240000 }
    ::= { cadSysMtceParams 10 }

cadSysMtceBasePingRate OBJECT-TYPE
    SYNTAX     Unsigned32 (750..2250)
    UNITS      "milliseconds"
    MAX-ACCESS read-write
